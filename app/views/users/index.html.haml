.Wrapper
  %header
    = render partial: 'header'
    .menuWrapper
      %ul.menuWrapper__itemMenuWrapper
        %li.menuWrapper__itemMenuWrapper__item.item__category
          = link_to "カテゴリー", "#"
          .menuWrapper__itemMenuWrapper__item__category
            %ul.parents
              - @categories.each do |category|
                %li.parent
                  =link_to "#{category.name}", "#"
                  %ul.children
                    - category.children.each do |child|
                      %li.child
                        =link_to "#{child.name}", "#"
                        %ul.grandChildren
                          - child.children.each do |gchild|
                            %li.grandChild
                              =link_to "#{gchild.name}", "#"

        %li.menuWrapper__itemMenuWrapper__item.item__brand
          = link_to "ブランド", "#"
          .menuWrapper__itemMenuWrapper__item__brand
            %ul
              %li
                =link_to "NIKE", "#"
              %li
                =link_to "adidas", "#"
              %li
                =link_to "asics", "#"
              %li
                =link_to "MIZUNO", "#"
              %li
                =link_to "シャネル", "#"
              %li
                =link_to "Tiffany", "#"
              %li
                =link_to "BVLGARI", "#"
              %li
                =link_to "GUCCI", "#"
              %li
                =link_to "エルメス", "#"
              %li
                =link_to "beams", "#"
              %li
                =link_to "TOMY", "#"
              %li
                =link_to "journalstandard", "#"
              %li
                =link_to "その他", "#"
      %ul.menuWrapper__userMenuWrapper
        = render partial: 'user'

  .MypageBody
    .SideList
      = link_to "#", class: "ListBtn" do
        %p 出品した商品  
      = link_to "#", class: "ListBtn" do
        %p いいねした商品 
      = link_to "#", class: "ListBtn" do
        %p 購入履歴
      = link_to "#", class: "ListBtn" do
        %p ポイント  
      = link_to "#", class: "ListBtn" do
        %p プロフィール 
      = link_to "#", class: "ListBtn" do
        %p 支払い方法    
      = link_to "#", class: "ListBtn" do
        %p ログアウト
    .MainField
      .UserBox
        .UserBox__UserName Username
        .UserBox__Number 出品数：0
      .SaleItems
        .SaleHead 出品した商品
        .SaleItemsStatus
          = link_to "#", class: "ItemsStatus" do
            %p 出品中
          = link_to "#", class: "ItemsStatus" do
            %p 取引中
          = link_to "#", class: "ItemsStatus" do
            %p 売却済
        .ItemsBox
          .item
            = link_to "#" do
              = image_tag "https://s3-ap-northeast-1.amazonaws.com/mercarimaster/uploads/captured_image/content/18/beef.JPG"
              %p beef
              %ul
                %li
                  1000円
                  %span
                    = icon('fas', 'star')
                    1
              %span.tax (税込)
          .item
            = link_to "#" do
              = image_tag "https://s3-ap-northeast-1.amazonaws.com/mercarimaster/uploads/captured_image/content/16/IMG_9072.JPG"
              %p moyashi
              %ul
                %li
                  900円
                  %span
                    = icon('fas', 'star')
                    2
              %span.tax (税込)
      .FavoriteItems
        .FavoriteHead いいね！一覧
        .ItemsBox
          .item
            = link_to "#" do
              = image_tag "https://s3-ap-northeast-1.amazonaws.com/mercarimaster/uploads/captured_image/content/16/IMG_9072.JPG"
              %p moyashi
              %ul
                %li
                  900円
                  %span
                    = icon('fas', 'star')
                    2
              %span.tax (税込)
          .item
