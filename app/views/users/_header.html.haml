.topWrapper
  .topWrapper__logo
    = link_to image_tag(asset_path("logo/logo.png")), root_path
  .topWrapper__searchForm
    = form_with url: "#" do |f|
      = f.text_field :search_word, placeholder: "キーワードから探す", class: "inputKeyword"
      %button{type: "submit"}
        = image_tag(asset_path("icon/icon-search1.png"))